{
  "name": "express-ts-rest-demo",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch main.ts",
    "build:docs": "tsx generate-docs.ts",
    "d1": "npx @redocly/cli preview-docs ./public/openapi.json",
    "d2": "npx @redocly/cli build-docs ./public/openapi.json --output=api-docs.html",
    "build": "tsup main.ts --format esm --sourcemap --clean",
    "es-build": "esbuild main.ts --bundle --platform=node --format=esm --packages=external --sourcemap --outfile=dist/main.js",
    "start": "node --enable-source-maps dist/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@orpc/client": "^1.13.2",
    "@orpc/openapi": "^1.13.2",
    "@orpc/server": "^1.13.2",
    "@orpc/zod": "^1.13.2",
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@redocly/cli": "^2.14.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "esbuild": "^0.27.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
